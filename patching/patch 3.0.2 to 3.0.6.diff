+                                return const SizedBox();
+                              }
+                              return Padding(
+                                padding: const EdgeInsets.only(top: 20.0),
+                                child: TextField(
+                                  controller: _ownerTextController,
+                                  readOnly: true,
+                                  decoration: InputDecoration(
+                                    labelText: S.of(context).name,
+                                    contentPadding: EdgeInsets.zero,
+                                  ),
