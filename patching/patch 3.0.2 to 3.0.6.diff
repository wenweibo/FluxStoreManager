+  bool isFieldReadOnly(int index) {
+    final config = _configs[index];
+    if (config == null) {
+      return false;
+    }
+
+    /// Disable edit only when the field has a default value.
+    if (!config.editable && config.defaultValue.isNotEmpty) {
+      return true;
+    }
+
+    return false;
+  }
+}
